import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
		String s=sc.nextLine();
	    String temp=s;
	    String s1="";
	    for(int i=s.length()-1;i>=0;i--)
	    {
	        s1+=s.charAt(i);
	    }
	    if(temp.equals(s1)){
	        System.out.println("Yes");
	    }
	    else{
	        System.out.println("No");
	    }
	}
}
